{
  /* Maximum number of concurrent instances of HandBrakeCLI that can run*/
  /* Currently maximum 1. Multi-instance is on the TODO list. */
  "Instances": 1,

  /* A set of tasks that can be run. Either by ID, or all*/
  "Tasks": [
    {
      "TaskId": 1,
      "Name": "Standard Benchmark 1080p -> 720p x264",
      "Description": "Emulating a user converting a high quality 1080p source to 720p for device playback and reduced filesize.",
      "SourceFile": "bbb.mp4",
      "CustomCommands": [
        {
          "CommandId": 1,
          "Name": "Run 1",
          "Command": "-i {source} -o {output_file} --preset=\"Fast 1080p30\" --no-deinterlace --no-decomb --stop-at seconds:20"
        },
        {
          "CommandId": 2,
          "Name": "Run 2",
          "Command": "-i {source} -o {output_file} --preset=\"Fast 1080p30\" --no-deinterlace --no-decomb --stop-at seconds:20"
        },
        {
          "CommandId": 3,
          "Name": "Run 3",
          "Command": "-i {source} -o {output_file} --preset=\"Fast 1080p30\" --no-deinterlace --no-decomb --stop-at seconds:20"
        }
      ]
    },
    {
      "TaskId": 2,
      "Name": "UHD Benchmark 4K -> 1080p x265",
      "Description": "Emulating a user converting a bluray to 1080p high quality for local computer playback.",
      "SourceFile": "bbb.mp4",
      "CustomCommands": [
        {
          "CommandId": 1,
          "Name": "Run 1",
          "Command": "-i {source} -o {output_file} --preset=\"H.264 MKV 2160p60\" --no-deinterlace --no-decomb --stop-at seconds:20"
        },
        {
          "CommandId": 2,
          "Name": "Run 2",
          "Command": "-i {source} -o {output_file} --preset=\"H.264 MKV 2160p60\" --no-deinterlace --no-decomb --stop-at seconds:20"
        },
        {
          "CommandId": 3,
          "Name": "Run 3",
          "Command": "-i {source} -o {output_file} --preset=\"H.264 MKV 2160p60\" --no-deinterlace --no-decomb --stop-at seconds:20"
        }
      ]
    }
  ]
}
